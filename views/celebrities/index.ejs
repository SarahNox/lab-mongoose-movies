<h1> Wannabes of the year! </h1>

<ul>
  <% celebrities.forEach((celebrity) => { %>
    <li>
      <%= celebrity.name %>
      <a href="/celebrities/<%= celebrity._id %>">Details</a>
      <a href="/celebrities/<%= celebrity._id %>/edit/">Edit</a>
      <a href="/celebrities/<%= celebrity._id %>/delete/">Delete</a>
    </li>
  <%  }) %>
  <a href="celebrities/new">New</a><br>
  <a href="movies">Switch to Movies</a>
</ul>
